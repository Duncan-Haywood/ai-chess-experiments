    "framer-motion": "^10.16.16",
    "react": "^18.2.0",
    "react-chartjs-2": "^5.2.0",
    "react-chessboard": "^4.3.4",
    "react-dom": "^18.2.0",
    "react-icons": "^4.12.0",
    "winston": "^3.11.0"
